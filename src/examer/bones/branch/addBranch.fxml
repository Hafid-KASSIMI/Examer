<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="12.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="examer.bones.branch.AddBranchController">
   <children>
      <Label layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308" text="%BRANCHES_SC">
         <styleClass>
            <String fx:value="body-title" />
            <String fx:value="bottom-half-bar" />
         </styleClass></Label>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="3.0">
         <children>
            <TextField fx:id="branchNameTF" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="infos-tf" HBox.hgrow="ALWAYS" />
            <Button fx:id="addBtn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" prefWidth="36.0" styleClass="icm" text="">
               <tooltip>
                  <Tooltip text="%ADD" />
               </tooltip></Button>
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="35.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="3.0">
         <children>
            <ComboBox fx:id="branchesCMB" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="%BRANCHES_LIST" HBox.hgrow="ALWAYS" />
            <Button fx:id="refreshBtn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" prefWidth="36.0" styleClass="icm" text="">
               <tooltip>
                  <Tooltip text="%REFRESH" />
               </tooltip></Button>
            <Button fx:id="updateBtn" layoutX="166.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" prefWidth="36.0" styleClass="icm" text="">
               <tooltip>
                  <Tooltip text="%UPDATE" />
               </tooltip></Button>
            <Button fx:id="deleteBtn" layoutX="166.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" prefWidth="36.0" styleClass="icm" text="">
               <tooltip>
                  <Tooltip text="%DELETE" />
               </tooltip></Button>
         </children>
      </HBox>
      <VBox fx:id="updateVB" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="12.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%RENAME_SC" />
            <HBox alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="3.0">
               <children>
                  <TextField fx:id="newBranchNameTF" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="infos-tf" HBox.hgrow="ALWAYS" />
                  <Button fx:id="doUpdateBtn" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="btn" text="%APPLY" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
